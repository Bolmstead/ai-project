{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": ["username", "password"],
  "properties": {
    "username": {
      "type": "string",
      "default": "",
      "title": "The username Schema",
      "examples": ["JohnDoe"]
    },
    "password": {
      "type": "string",
      "default": "",
      "title": "The password Schema",
      "examples": ["1234abcd"]
    },
    "isClient": {
      "type": "boolean",
      "default": false,
      "title": "The isClient Schema",
      "examples": [false]
    }
  },
  "examples": [
    {
      "username": "JohnDoe",
      "password": "1234abcd",
      "isClient": false
    }
  ]
}
